{
  "first-header-h1": false,
  "first-line-h1": false,
  "line-length": false,
  "no-duplicate-header": false,
  "no-emphasis-as-header": false,
  "no-inline-html": {
    "allowed_elements": [
      "a",
      "br",
      "img",
      "sup"
    ]
  },
  "no-multiple-blanks": false,
  "no-trailing-punctuation": {
    "punctuation": ".,;:!"
  },
  "ol-prefix": {
    "style": "ordered"
  },
  "ul-indent": {
    "indent": 4
  }
}